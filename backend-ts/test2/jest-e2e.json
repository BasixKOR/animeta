{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": "\\.spec\\.ts$",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)": "<rootDir>/../dist/backend-ts/src/$1"
  },
  "globalSetup": "<rootDir>/utils/globalSetup.ts",
  ".globalTeardown": "<rootDir>/../node_modules/@databases/pg-test/jest/globalTeardown"
}
