{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "main.js",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@animeta/tooling",
        "options": {
          "sourceRoot": "src",
          "endpointsDir": "endpoints",
          "distDir": "dist",
          "clientTypesOutput": "../web/shared/client.d.ts"
        }
      }
    ]
  }
}
